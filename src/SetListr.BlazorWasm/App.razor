<Router AppAssembly="@typeof(App).Assembly" AdditionalAssemblies="[typeof(SetListr.Razor.Routes).Assembly]">
  <Found Context="routeData">
    <RouteView RouteData="@routeData" DefaultLayout="@typeof(SetListr.Razor.Shared.MainLayout)" />
  </Found>
  <NotFound>
    <PageTitle>Not found</PageTitle>
    <LayoutView Layout="@typeof(SetListr.Razor.Shared.MainLayout)">
      <p role="alert">Sorry, there's nothing at this address.</p>
    </LayoutView>
  </NotFound>
</Router>