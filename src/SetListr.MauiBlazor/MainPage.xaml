<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:SetListr.MauiBlazor"
             xmlns:razor="clr-namespace:SetListr.Razor;assembly=SetListr.Razor"
             x:Class="SetListr.MauiBlazor.MainPage">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type razor:Routes}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
